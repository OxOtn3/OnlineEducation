<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlineeducation">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineEducation">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".coursesPage.CoursesPageActivity" />
        <activity android:name=".courseIntroductionPage.CourseIntroductionPageActivity" />
        <activity android:name=".VideoPlayPage.VideoPlayActivity" />
        <activity android:name=".teacherHomepage.TeacherHomepageActivity" />
        <activity android:name=".mineFragmentPages.BoughtCoursesPageActivity" />
        <activity android:name=".momentPage.MomentPage" />
        <activity android:name=".momentUserPage.MomentUserPageActivity" />
        <activity android:name=".mineFragmentPages.CoursesOperationPage" />
        <activity android:name=".mineFragmentPages.OpenNewCoursesPage" />
        <activity android:name=".mineFragmentPages.KeywordPage" />
        <activity android:name=".courseIntroductionPage.CoursesCommentPage" />
        <activity android:name=".teacherHomepage.TeacherCommentPage" />
        <activity android:name=".courseIntroductionPage.PaymentPage" />
        <activity android:name=".SearchPage" />
        <activity android:name=".SignUpPage" />
        <activity android:name=".TeachersPage" />
        <activity android:name=".LivePage" />
        <activity android:name=".SearchKeywordPage" />
        <activity android:name=".SignUpInfomationCollectionPage" />

    </application>

</manifest>