<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlineeducation">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineEducation">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".coursesPage.CoursesPageActivity" ></activity>
        <activity android:name=".courseIntroductionPage.CourseIntroductionPageActivity"></activity>
        <activity android:name=".VideoPlayPage.VideoPlayActivity"></activity>
        <activity android:name=".teacherHomepage.TeacherHomepageActivity"></activity>
        <activity android:name=".mineFragmentPages.BoughtCoursesPageActivity"></activity>
        <activity android:name=".momentPage.MomentPage"></activity>
        <activity android:name=".momentUserPage.MomentUserPageActivity"></activity>
        <activity android:name=".mineFragmentPages.CoursesOperationPage"></activity>
        <activity android:name=".mineFragmentPages.OpenNewCoursesPage"></activity>
        <activity android:name=".mineFragmentPages.KeywordPage"></activity>
        <activity android:name=".courseIntroductionPage.CoursesCommentPage"></activity>
        <activity android:name=".teacherHomepage.TeacherCommentPage"></activity>
        <activity android:name=".courseIntroductionPage.PaymentPage"></activity>
        <activity android:name=".SearchPage"></activity>
        <activity android:name=".SignUpPage"></activity>
    </application>

</manifest>